#: import FadeTransition kivy.uix.screenmanager.FadeTransition

ScreenManagement:
    transition: FadeTransition()
    MainScreen:
    AnotherScreen:

<MainScreen>:
    name: "main"

    FloatLayout:
        Button:
            on_release: app.root.current = "other"
            text: "Start Exercise"
            font_size: 50

<AnotherScreen>:
    name: "other"

    FloatLayout:
        TouchInput
        Button:
            color: 0,1,0,1
            size_hint: 0.3, 0.2
            text: "Home"
            font_size: 25
            pos_hint: {"right":1,"top":1}





